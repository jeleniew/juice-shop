<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="50dp" android:width="50dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/background_color" />
        </shape>
    </item>
    <item android:top="5dp" android:bottom="15dp" android:right="10dp" android:left="10dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:top="7dp"  android:bottom="10dp" android:right="12dp" android:left="12dp">
        <shape android:shape="oval">
            <solid android:color="@color/background_color" />
        </shape>
    </item>
    <item android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/background_color" />
        </shape>
    </item>

    <item android:top="10dp" android:bottom="5dp" android:left="12dp" android:right="35dp">
        <rotate android:fromDegrees="-5">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>
    <item android:top="10dp" android:bottom="5dp" android:left="20dp" android:right="27dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:top="10dp" android:bottom="5dp" android:left="27dp" android:right="20dp">
        <shape
            android:layout_height="wrap_content"
            android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:top="10dp" android:bottom="5dp" android:left="35dp" android:right="12dp">
        <rotate android:fromDegrees="5">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>
    <item android:top="10dp" android:bottom="37dp" android:left="8dp" android:right="8dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:top="42dp" android:bottom="5dp" android:left="13dp" android:right="13dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>
